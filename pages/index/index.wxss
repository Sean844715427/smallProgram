/* 轮播样式 */
.swiper {
  height: 300rpx;
  width: 750rpx;
}

.swiper-image {
  width: 100%;
  height: 100%;
}

/* 文章 */
.article{
  padding: 30rpx;
}
.artmode{
  background-color: #b5beca;
  border-radius: 10rpx;
  box-shadow: #b5beca 5rpx 5rpx 5rpx;
}
.line{
  width: 100%;
  height: 1rpx;
  background-color: #b5beca;
}
.txtBox{
  display: flex;
  justify-content: space-between;
}


